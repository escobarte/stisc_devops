93.3
#Installign Zabbix-Agent2 on Server
wget https://repo.zabbix.com/zabbix/7.0/ubuntu/pool/main/z/zabbix-release/zabbix-release_7.0-1+ubuntu24.04_all.deb
sudo dpkg -i zabbix-release_7.0-1+ubuntu24.04_all.deb
sudo apt update
sudo apt install zabbix-agent2 -y

#Coniguring on Agent Server
sudo nano /etc/zabbix/zabbix_agent2.conf
Server=10.100.93.7
ServerActive=10.100.93.7
Hostname=app01
sudo systemctl enable zabbix-agent2
sudo systemctl restart zabbix-agent2
sudo systemctl status zabbix-agent2

!!! On Nginx1 (93.3) i am using IPTABLES !!!
sudo iptables -L INPUT --line-numbers
sudo iptables -I INPUT -p tcp -s 10.100.93.7 --dport 10050 -j ACCEPT
sudo ss -tuln | grep 10050