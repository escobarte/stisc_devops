#Filtering
history | awk '{$1=$2=$3=""; print $0}' | sed 's/^ *//'
find /path/to/source/folder/ -maxdepth 1 -type f -name "*Gemeni*" -exec mv {} /path/to/destination/folder/ \; #search by name contains this word and move it to other folder
